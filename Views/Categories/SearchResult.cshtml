@model IEnumerable<mvcdemo.Category>

<table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CatCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CatDesc)
            </th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CatCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CatDesc)
            </td>
        </tr>
    }
    
    </table>
